<body>
    <nav class="sidebar">
        
        <div class="container_img_defaul">
            <img src="/resources/img/shop/logo.9376039b.png" alt="" class="img_shop_default">
            <div class="text_menu">Cửa hàng</div>
        </div>
        <ul>   
            <li class="emply">
                <a href="/" id="back-to-home">
                    <i class="fas fa-home icon_func"></i>
                    <div class="text_func_menu">
                        Trang chủ
                    </div>
                </a>
            </li> 
            <li class="emply">
                <a href="/shops/incomes">
                    <i class="fas fa-dollar icon_func"></i>
                    <div class="text_func_menu">
                        Doanh thu
                    </div>
                </a>
            </li>
            <li class="emply">
                <a href="/shops/bills" class="feat-btn">
                    <i class="fas fa-shopping-cart icon_func"></i>
                    <div class="text_func_menu">
                        Đơn hàng
                    </div>
                </a>
            </li>
            <li class="emply">
                <a href="#" class="product-btn">
                    <i class="fab fa-product-hunt icon_func"></i>
                    <div class="text_func_menu">
                        Sản phẩm
                    </div>
                    <span class="fas fa-caret-down second"></span>
                </a>
                <ul class="product-show">
                    <li>
                        <a href="/shops/products">Danh sách sản phẩm</a>
                    </li>
                    <li>
                        <a href="/shops/new">Tạo mới sản phẩm</a>
                    </li>
                </ul>
            </li>
        </ul>
        <script>
            $('.feat-btn').click(function(){
                $('nav ul .feat-show').toggleClass("show");
                $('nav ul .first').toggleClass("rotate");
                $('nav ul .icon_func').toggleClass("stay")
            });
            $('.product-btn').click(function(){
                $('nav ul .product-show').toggleClass("show1");
                $('nav ul .second').toggleClass("rotate");
                $('nav ul .icon_func').toggleClass("stay")
            });
            $('nav ul .emply').click(function(){
                $(this).addClass("active").siblings().removeClass("active");
            });
        </script>

    </nav>
